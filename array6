//Program to print the elements of an array in reverse order
#include<stdio.h>
int main()
{
int n,i;
printf("Enter range of the array: ");
scanf("%d",&n);
int a[n];
for(i=0;i<n;i++)
{
printf("Enter element: ");
scanf("%d",&a[i]);
}
printf("Array in reverse order is: ");
for(i=(n-1);i>=0;i--)
printf("\n%d",a[i]);
return 0;
}
