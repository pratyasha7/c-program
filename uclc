//C program to convert an uppercase character to a lowercase character and vise versa

#include <stdio.h>

int main() {
  char ch;
  printf("Enter a character: ");
  scanf("%c", &ch);
  if (ch >= 'A' && ch <= 'Z') {
        ch+=32;
    printf("Lowercase %c\n", ch);
  }
else if (ch >= 'a' && ch <= 'z') {
      ch=ch-32;
    printf("Uppercase %c\n",ch);
  }
   else 
    printf("%c is not a letter\n",ch);

  return 0;
}
