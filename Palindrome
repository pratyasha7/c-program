//Determine whether the number is Palindrome or not
#include<stdio.h>
int main()
{
int rev=0,no,n;
printf("Enter a number: ");
scanf("%d",&no);
n=no;
while(no>=1)
{
rev=(rev*10)+(no%10);
no=(no/10);
}
if(rev==n)
printf("\nIt is a palindrome number");
else
printf("\nIt's not a palindrome number");
return 0;
}
