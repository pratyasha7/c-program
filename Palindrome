//Determine whether the number is Palindrome or not
#include<stdio.h>
int main()
{
int no,rev=0,n;
printf("Enter an integer x: ");
scanf("%d",&no);
n=no;
while(no!=0)
{
rev= rev*10+(no%10);
no/=10;
}
if(rev==n && rev>0)
printf("It's a palindrome number");
else
printf("It's not a palindrome number");
return 0;
}
