//Check whether a number is prime or composite
#include<stdio.h>
int main()
{
    int num,i=1,count=0,n;
    printf("Enter a number: ");
    scanf("%d",&num);
    n=num;
    while(i<=num)
    {
        if (num%i==0)
            count++;
            i++;
    }

if (n==0 || n==1)
    printf("The number is neither prime nor composite");
else if(count==2)
    printf("It's a prime number");
else
    printf("It is a composite number");
return 0;
}
