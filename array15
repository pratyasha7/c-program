//Displaying the elements of an array in ascending order using selection sorting
#include<stdio.h>
int main()
{
    int a[10],i,j,temp;
    for(i=0;i<10;i++){
        printf("Enter element: ");
    scanf("%d",&a[i]);
    }
    for(i=0;i<9;i++){
        for(j=i+1;j<10;j++){
                if(a[i]>a[j]){
            temp=a[j];
            a[j]=a[i];
            a[i]=temp;
                }
        }
        }
        printf("The ordered string is");
        for(i=0;i<10;i++)
            printf("\n%d",a[i]);
        return 0;

}
