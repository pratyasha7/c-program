//Printing alternate elements of an array by taking the range from the user
#include<stdio.h>
int main()
{
int n,i;
printf("Enter the range of the array: ");
scanf("%d",&n);
int a[n];
for(i=0;i<n;i++)
{
printf("Enter element: ");
scanf("%d",&a[i]);
}
printf("The elements of the array are: ");
for(i=0;i<n;i++)
printf("\n%d",a[i]);
return 0;
}
