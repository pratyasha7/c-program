// To count the total number of even and odd numbers in an array
#include<stdio.h>
int main()
{
int a[10],i,even=0,odd=0;
for(i=0;i<10;i++)
{
printf("Enter number: ");
scanf("%d",&a[i]);
}
for(i=0;i<10;i++)
{
if(a[i]%2==0)
even++;
else
odd++;
}
printf("\nTotal even numbers: %d",even);
printf("\nTotal odd numbers: %d",odd);
return 0;
}
